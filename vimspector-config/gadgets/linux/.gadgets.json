{
  "adapters": {
    "multi-session": {
      "host": "${host}",
      "port": "${port}"
    },
    "vscode-cpptools": {
      "attach": {
        "pidProperty": "processId",
        "pidSelect": "ask"
      },
      "command": [
        "${gadgetDir}/vscode-cpptools/debugAdapters/OpenDebugAD7"
      ],
      "configuration": {
        "args": [],
        "cwd": "${workspaceRoot}",
        "environment": [],
        "type": "cppdbg"
      },
      "name": "cppdbg"
    }
  }
}